*/
		
		TestCompare test1 = new TestCompare();
		TestCompare test2 = new TestCompare();
		System.out.println("test1的str值：    "+str);
		System.out.println("test1的str2值：  "+test1.str2);
		System.out.println("test2的str值：    "+str);
