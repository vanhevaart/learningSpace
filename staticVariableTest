	public String str2="ubi";
	
//	构造方法
	public TestCompare() {
		
	}

	public TestCompare(String str1, String str2) {
		str ="test";
		this.str2="accreed";
		
	}
	
//	设置str的方法
	
	public void setstr(String str) {
		this.str2="str";
	}
	
	public void setstr2(String str2) {
		this.str2="hezheng";
	}
	
	public static void main(String[] args) {
/*
 
		int x=1;
		int y=2;
		double m=0.3;
		float n=0.3f;
